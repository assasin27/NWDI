{
  "name": "t-er-backend",
  "version": "1.0.0",
  "description": "Backend for T-ER project",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn src/app.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "migrate": "node-pg-migrate",
    "migrate:up": "npm run migrate up",
    "migrate:down": "npm run migrate down",
    "validate": "tsc --noEmit",
    "prepare": "husky install",
    "postinstall": "npm run build",
    "docker:build": "docker build -t farmfresh-api .",
    "docker:run": "docker run -p 3000:3000 farmfresh-api"
  },
  "keywords": [
    "django",
    "backend",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@sentry/node": "^10.11.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/newrelic": "^9.14.8",
    "@types/node-cron": "^3.0.11",
    "@types/opossum": "^8.1.9",
    "@types/pg": "^8.15.5",
    "@types/redis": "^4.0.10",
    "@types/sanitize-html": "^2.16.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.11.0",
    "compression": "^1.8.1",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "newrelic": "^13.3.2",
    "node-cron": "^4.2.1",
    "opossum": "^9.0.0",
    "pg": "^8.16.3",
    "rate-limit-redis": "^4.2.2",
    "redis": "^5.8.2",
    "sanitize-html": "^2.17.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.3",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "eslint": "^9.35.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jsonwebtoken": "^9.0.2",
    "lint-staged": "^16.1.6",
    "node-pg-migrate": "^8.0.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prettier": "^3.6.2",
    "stripe": "^18.4.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2"
  }
}
